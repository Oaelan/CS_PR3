<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="org.hj.mapper.sub.ProductRegister_Mapper">
  
  	<!-- 상품 등록시 상품 코드 가져오는 sql -->
  	<select id="addPno" parameterType="org.hj.model.Product_manufacturingDto" resultType="int">
		select p_no from Supplier.productCode
		where p_name = #{p_name};
	</select>

</mapper>