<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper namespace="org.hj.mapper.LoginM_Mapper">
  
  <select id="loginM" parameterType="org.hj.model.UserDto" resultType="org.hj.model.MasterDto">
  select m_id from Master
  where m_id = #{u_id} and m_pw = #{u_pw};
  </select>
  
  
  
  
  
  
